    "start": "npm run build",
    "html": "node build/copyhtml.js",
    "jest": "jest --config test/unit/jest.conf.js --coverage --color",
    "test": "npm run jest",
    "testrun": "jest --config test/unit/jest.conf.js --testNamePattern \"#run\" --color"
